# The train/test net protocol buffer definition
net: "./models/merged/merged.prototxt"

# optimization method
type: "Adam" 

# max_iter specifies the number of training iterations
max_iter: 2000

# The base learning rate of the network.
base_lr: 0.1

# The learning rate policy
lr_policy: "step"
gamma: 0.7
stepsize: 1000

# Display every 50 iterations
display: 50

# solver mode: CPU or GPU
solver_mode: GPU
