layer {
  name: "data"
  type: "NoiseData"
  top: "data"
  noise_data_param {
    batch_size: 2
    channels: 3
    spatial_size: 6
    distribution: "gaussian"
    mu: 0
    sigma: 1
  }
}
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "data"
  top: "conv1"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "conv_relu1"
  type: "ReLU"
  bottom: "conv1"
  top: "conv1"
}
layer {
  name: "gramian"
  type: "Gramian"
  bottom: "conv1"
  top: "gramian"
  gramian_param {
    normalize_output: true
  }
}
