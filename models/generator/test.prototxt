layer {
  name: "input"
  type: "Input"
  top: "dummydata"
  input_param {
    shape {
      dim: 2
      dim: 1
      dim: 1
      dim: 4096
    }
  }
}
layer {
  name: "loss"
  type: "EuclideanLoss"
  bottom: "gramian"
  bottom: "dummydata"
  top: "loss"
}
