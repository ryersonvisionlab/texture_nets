layer {
  name: "data"
  type: "ImageData"
  top: "data"
  top: "label"
  image_data_param {
    source: "../../../data/textures/textures.txt"
    batch_size: 1
  }
}
layer {
  name: "upsample"
  type: "Deconvolution"
  bottom: "data"
  top: "upsample"
  convolution_param {
    kernel_size: 2
    stride: 2
    num_output: 3
    group: 3
    pad: 1
    weight_filler: { 
      type: "constant"
      value: 1
    }
    bias_term: false
  }
  param { lr_mult: 0 decay_mult: 0 }
}
